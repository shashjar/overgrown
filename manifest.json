{
  "manifest_version": 3,
  "name": "Plants that help you manage your tabs!",
  "version": "1.0",
  "icons": {
    "128": "images/logo.png"
  },
  "action": {
    "default_popup": "overgrown.html"
  },
  "host_permissions": ["https://developer.chrome.com/*", "http://*/*", "https://*/*"],
  "permissions": ["tabs", "activeTab", "background", "storage"],
  "background": {
    "service_worker": "background.js"
  }
}
